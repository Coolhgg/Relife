# Staging Environment Configuration
NODE_ENV=production
VITE_APP_ENV=staging
VITE_APP_VERSION=2.0.0-staging

# Database (Staging)
VITE_SUPABASE_URL=https://your-staging-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_staging_anon_key

# Analytics & Monitoring (Staging)
VITE_POSTHOG_KEY=phc_staging_key_here
VITE_POSTHOG_HOST=https://app.posthog.com
VITE_SENTRY_DSN=https://staging@sentry.io/project
VITE_SENTRY_ENVIRONMENT=staging

# Performance Monitoring
VITE_PERFORMANCE_MONITORING=true
VITE_PERFORMANCE_ENDPOINT=https://api-staging.relife.app/api/performance
VITE_ANALYTICS_ENDPOINT=https://api-staging.relife.app/api/analytics

# Feature Flags (Staging - Production-like)
VITE_ENABLE_SESSION_RECORDING=true
VITE_ENABLE_HEATMAPS=true
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_OFFLINE_SUPPORT=true
VITE_ENABLE_VOICE_SYNTHESIS=true

# Staging Deployment
VITE_APP_DOMAIN=staging.relife.app
VITE_API_BASE_URL=https://api-staging.relife.app
VITE_CDN_URL=https://cdn-staging.relife.app
VITE_ENABLE_HTTPS=true

# Security Settings
VITE_RATE_LIMIT_ENABLED=true

# External Services (Staging Keys)
VITE_DATADOG_CLIENT_TOKEN=staging_datadog_token
VITE_NEW_RELIC_ACCOUNT_ID=staging_newrelic_account

# Performance Thresholds (Production-like)
VITE_PERFORMANCE_LCP_THRESHOLD=2800
VITE_PERFORMANCE_FID_THRESHOLD=120
VITE_PERFORMANCE_CLS_THRESHOLD=0.12
VITE_PERFORMANCE_MEMORY_THRESHOLD=60

# Health Monitoring
VITE_HEALTH_CHECK_INTERVAL=30000
VITE_UPTIME_ROBOT_KEY=staging_uptime_key

# Build Settings
VITE_BUILD_ANALYZE=true
VITE_BUILD_SOURCEMAPS=false
VITE_BUILD_MINIFY=true
VITE_BUILD_TARGET=es2020