# Performance Monitoring Configuration for Development

# React Performance Profiler
REACT_APP_PERFORMANCE_PROFILING=true
REACT_APP_PROFILING_LOG_LEVEL=summary
REACT_APP_SLOW_RENDER_THRESHOLD=16
REACT_APP_SHOW_PERF_DASHBOARD=true
REACT_APP_PERF_REPORTING=false

# Performance Analytics Endpoint
REACT_APP_ANALYTICS_ENDPOINT=http://localhost:3001/api

# k6 Load Testing Configuration
BASE_URL=http://localhost:3000
API_URL=http://localhost:3001/api
STRESS_LEVEL=medium
SOAK_DURATION=10

# Lighthouse CI Configuration
LHCI_GITHUB_APP_TOKEN=
LHCI_SERVER_URL=

# Performance Monitoring
PERFORMANCE_MONITORING_ENABLED=true
PERFORMANCE_THRESHOLD_P95=300
PERFORMANCE_THRESHOLD_ERROR_RATE=0.01