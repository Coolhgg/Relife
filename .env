# Core App Configuration
VITE_APP_NAME="Relife"
VITE_API_BASE_URL="https://api.relife.app"
VITE_APP_VERSION="1.0.0"

# Supabase Configuration
VITE_SUPABASE_URL="your-supabase-url"
VITE_SUPABASE_ANON_KEY="your-supabase-anon-key"

# Cloud Sync Configuration
VITE_CLOUD_SYNC_ENABLED=true
VITE_SYNC_INTERVAL=30000

# Analytics Configuration  
VITE_POSTHOG_KEY="your-posthog-key"
VITE_ANALYTICS_ENABLED=true

# Development Configuration
NODE_ENV=development
VITE_DEV_MODE=true

# Optional External Service Keys (leave empty if not using)
VITE_OPENAI_API_KEY=""
VITE_ELEVENLABS_API_KEY=""
VITE_STRIPE_PUBLISHABLE_KEY=""
VITE_SENTRY_DSN=""
VITE_DATADOG_RUM_APPLICATION_ID=""
VITE_DATADOG_RUM_CLIENT_TOKEN=""
VITE_NEWRELIC_LICENSE_KEY=""

# Push Notification Configuration
VITE_VAPID_PUBLIC_KEY=""
VAPID_PRIVATE_KEY=""

# Security Configuration
VITE_ENABLE_SECURITY_HEADERS=true
VITE_CSRF_TOKEN=""

# PWA Configuration
VITE_PWA_ENABLED=true
VITE_OFFLINE_MODE=true

# Debugging
VITE_DEBUG_MODE=false
VITE_VERBOSE_LOGGING=false