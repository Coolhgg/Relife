{
  "extends": "lighthouse:default",
  "settings": {
    "onlyCategories": [
      "pwa",
      "performance",
      "accessibility"
    ],
    "skipAudits": [
      "redirects-http"
    ]
  },
  "audits": [
    "installable-manifest",
    "splash-screen",
    "themed-omnibox",
    "content-width",
    "viewport",
    "apple-touch-icon",
    "maskable-icon",
    "service-worker",
    "works-offline",
    "offline-start-url"
  ]
}