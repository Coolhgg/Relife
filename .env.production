# Production Environment Configuration
NODE_ENV=production
VITE_APP_ENV=production
VITE_APP_VERSION=2.0.0

# Database (Production)
VITE_SUPABASE_URL=https://your-production-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_production_anon_key

# Analytics & Monitoring (Production)
VITE_POSTHOG_KEY=phc_production_key_here
VITE_POSTHOG_HOST=https://app.posthog.com
VITE_SENTRY_DSN=https://production@sentry.io/project
VITE_SENTRY_ENVIRONMENT=production

# Performance Monitoring
VITE_PERFORMANCE_MONITORING=true
VITE_PERFORMANCE_ENDPOINT=https://api.relife.app/api/performance
VITE_ANALYTICS_ENDPOINT=https://api.relife.app/api/analytics

# Feature Flags (Production)
VITE_ENABLE_SESSION_RECORDING=true
VITE_ENABLE_HEATMAPS=true
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_OFFLINE_SUPPORT=true
VITE_ENABLE_VOICE_SYNTHESIS=true

# Production Deployment
VITE_APP_DOMAIN=relife.app
VITE_API_BASE_URL=https://api.relife.app
VITE_CDN_URL=https://cdn.relife.app
VITE_ENABLE_HTTPS=true

# Security Settings
VITE_RATE_LIMIT_ENABLED=true

# External Monitoring Services (Production Keys)
VITE_DATADOG_CLIENT_TOKEN=production_datadog_token
VITE_NEW_RELIC_ACCOUNT_ID=production_newrelic_account
VITE_AMPLITUDE_API_KEY=production_amplitude_key

# Performance Thresholds (Strict Production)
VITE_PERFORMANCE_LCP_THRESHOLD=2500
VITE_PERFORMANCE_FID_THRESHOLD=100
VITE_PERFORMANCE_CLS_THRESHOLD=0.1
VITE_PERFORMANCE_MEMORY_THRESHOLD=50

# Health & Uptime Monitoring
VITE_HEALTH_CHECK_INTERVAL=60000
VITE_UPTIME_ROBOT_KEY=production_uptime_key

# Notification Settings
VITE_VAPID_PUBLIC_KEY=production_vapid_public_key
VITE_FIREBASE_CONFIG={"apiKey":"...","authDomain":"..."}

# Build Settings (Optimized for Production)
VITE_BUILD_ANALYZE=false
VITE_BUILD_SOURCEMAPS=false
VITE_BUILD_MINIFY=true
VITE_BUILD_TARGET=es2020

# CDN and Caching
VITE_CACHE_STRATEGY=aggressive
VITE_ASSET_OPTIMIZATION=true